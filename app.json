{
  "expo": {
    "name": "eRestau",
    "slug": "eRestau",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/eRestau-icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/images/eRestau-splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/eRestau-adaptative-icon.png",
        "backgroundColor": "#ffffff"
      },
      "googleServicesFile": "./google-services.json",
      "package": "com.alziron.e_restau",
      "permissions": [
        "android.permission.RECORD_AUDIO",
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION",
        "android.permission.FOREGROUND_SERVICE"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "Allow E-Restou to access your storage."
        }
      ],
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow E-Restou to use your location."
        }
      ],
      [
        "expo-notifications",
        {
          "icon": "./assets/images/eRestau-icon.png",
          "color": "#ffffff",
          "sounds": [
            "./assets/sounds/notification.wav"
          ]
        }
      ],
      "@react-native-firebase/app",
      [
        "expo-build-properties",
        {
          "android": {
            "usesCleartextTraffic": true
          }
        }
      ],
      "expo-localization"
    ],
    "extra": {
      "eas": {
        "projectId": "fae1235c-d03a-419a-a70c-39a50fd7018c"
      }
    }
  }
}
