{
  "expo": {
    "name": "rms-app",
    "slug": "rms-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/alziron-logo.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },"plugin":[
      "expo-image-picker",
      {
        "photosPermission": "Allow E-Restou to access your storage."
      }, "expo-location",
      {
        "locationAlwaysAndWhenInUsePermission": "Allow E-Restou to use your location."
      }, "expo-notifications",
      {
        "icon": "./assets/favicon.png",
        "color": "#ffffff",
        "sounds": [
          "./assets/sounds/notification.wav"
        ]
      },"@react-native-firebase/app"
    ]
  }
}
